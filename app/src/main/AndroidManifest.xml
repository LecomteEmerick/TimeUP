<?xml version="1.0" encoding="utf-8"?>
<manifest
    xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.lechot.esgi.timeup">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".PlayerActivity"></activity>
        <activity android:name=".TeamViewActivity"></activity>
        <activity android:name=".CategoryActivity"></activity>
        <activity android:name=".MainScreenGameActivity"></activity>

        <uses-sdk android:minSdkVersion="14" android:targetSdkVersion="16" />
    </application>

</manifest>